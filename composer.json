{
  "name": "mimachh/guardians",
  "description": "Handle multi user roles in your Laravel app easily",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "version": "0.1.5",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Mimachh\\Guardians\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimachh\\Guardians\\Database\\Factories\\": "/database/factories/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mimachh\\Guardians\\GuardiansServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "Mimachh",
      "email": "mimach.dev@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.9"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23"
  }
}
